{
    "bracketSpacing": true,
    "printWidth": 80,
    "proseWrap": "preserve",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "tabWidth": 4,
    "useTabs": true,
    "parser": "typescript",
    "arrowParens": "always",
    "requirePragma": true,
    "insertPragma": true,
    "endOfLine": "lf"
}